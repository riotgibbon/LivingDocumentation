<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>
  <specFlow>
    
   <!--For additional details on SpecFlow configuration options see http://go.specflow.org/doc-config--> 
 
    <trace   listener="SpecFlowLivingDoc.SpecFlowLogger, SpecFlowLivingDoc"  />

  </specFlow>

  <appSettings>
    <add key="TargetProcessAPI" value="http://livingdoc.tpondemand.com/"/>
    <add key="UserName" value="livingdocbot"/>
